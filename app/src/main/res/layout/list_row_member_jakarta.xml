<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/about_play_store_color"
    android:orientation="vertical"
    android:padding="14dp">

    <TextView
        android:id="@+id/idJakarta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="4sp"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="Nama"
        android:textColor="@color/about_background_color" />

    <TextView
        android:id="@+id/namamemberJakarta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView16"
        android:layout_alignBottom="@+id/textView16"
        android:layout_alignLeft="@+id/nopungmemberJakarta"
        android:layout_alignStart="@+id/nopungmemberJakarta"
        android:textColor="#000000"
        android:textSize="15sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView16"
        android:layout_alignStart="@+id/textView16"
        android:layout_below="@+id/namamemberJakarta"
        android:text="Nopung"
        android:textColor="@color/about_background_color" />

    <TextView
        android:id="@+id/nopungmemberJakarta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/namamemberJakarta"
        android:layout_marginLeft="4dp"
        android:layout_toEndOf="@+id/textView20"
        android:layout_toRightOf="@+id/textView20"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Small"
        android:textColor="#fa7705"
        android:textSize="15sp"
        android:textStyle="bold" />


    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView18"
        android:layout_alignStart="@+id/textView18"
        android:layout_below="@+id/textView18"
        android:text="Regional"
        android:textColor="@color/about_background_color" />

    <TextView
        android:id="@+id/regionmemberJakarta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView20"
        android:layout_marginBottom="2dp"
        android:layout_marginLeft="4dp"
        android:layout_toEndOf="@+id/textView20"
        android:layout_toRightOf="@+id/textView20"
        android:textColor="#ea1414"
        android:textSize="15sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/regionmemberJakarta"
        android:text="Status"
        android:textColor="@color/about_background_color" />

    <TextView
        android:id="@+id/statusmemberJakarta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/regionmemberJakarta"
        android:layout_alignStart="@+id/regionmemberJakarta"
        android:layout_below="@+id/regionmemberJakarta"
        android:textColor="#231919"
        android:textSize="15sp"
        android:textStyle="bold|italic" />
</RelativeLayout>
